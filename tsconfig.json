{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "20.1.0",
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "lib": ["ESNext"],
    "strict": true,
    "rootDir": "./src",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "sourceMap": true
  },
  "exclude": ["node_modules"]
}
